Create database hackathon;
Use hackathon;

Create table info
(
Id int primary key,
Name varchar(20),
Age int,
Suggestion varchar(100)
);

Create table pulse
(
Time int primary key,
Pulse int
);

Create table Oxygen 
(
Time int primary key,
Oxygen int
);

Create table Motion 
(
Time int primary key,
Motion int
);

Create table Temp
(
Time int primary key,
Temp int
);
